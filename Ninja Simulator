local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xxsfytd/Aaaaa/main/Aaaaa"))()
local Window = Library.CreateLib("TITLE", "DarkTheme")

local Tab = Window:NewTab("Auto Insta Kill Boss")
local Section = Tab:NewSection("Dojo World 1")
Section:NewToggle("Trainee", "Required 100", function(state)
    if state then
        _G.loop = true
        while _G.loop == true do wait()
        local args = {
    [1] = "Area1",
    [2] = 1
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("FightOver"):FireServer(unpack(args))

end
    else
        _G.loop = false
        while _G.loop == true do wait()
        local args = {
    [1] = "Area1",
    [2] = 1
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("FightOver"):FireServer(unpack(args))

end
    end
end)

